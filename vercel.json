{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "files": [
      "markdownFiles/**/*", 
      ".next/**/*",        
      "public/**/*",       
      "api/**/*"           
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  